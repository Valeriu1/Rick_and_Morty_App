<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="600"
        motion:autoTransition="jumpToStart">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/imageButton"
               motion:framePosition="30"
               android:rotationX="0"
               android:scaleX="1.4"
               android:scaleY="1.4"
               android:rotation="0"
               android:rotationY="5" />
           <KeyAttribute
               motion:motionTarget="@+id/imageButton"
               motion:framePosition="95"
               android:rotation="10"
               android:scaleY="0.8" />
           <KeyAttribute
               motion:motionTarget="@+id/imageButton"
               motion:framePosition="70"
               android:rotation="-20"
               android:scaleX="0.9"
               android:scaleY="0.9" />
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/imageButton"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="84dp" />
        <Constraint
            android:id="@+id/status"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            motion:layout_constraintBottom_toTopOf="@+id/imageButton"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/species"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/name"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/number"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            motion:layout_constraintHorizontal_bias="0.044"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/name"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.006"
            motion:layout_constraintTop_toBottomOf="@+id/imageButton"
            motion:layout_constraintHorizontal_bias="0.0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageButton"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="84dp" />
        <Constraint
            android:id="@+id/status"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            motion:layout_constraintBottom_toTopOf="@+id/imageButton"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/species"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/name"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/number"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            motion:layout_constraintHorizontal_bias="0.044"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/name"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.006"
            motion:layout_constraintTop_toBottomOf="@+id/imageButton"
            motion:layout_constraintHorizontal_bias="0.0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/anotherStart" >
        <Constraint
            android:id="@+id/imageButton"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="84dp" />
        <Constraint
            android:id="@+id/status"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            motion:layout_constraintBottom_toTopOf="@+id/imageButton"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/species"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/name"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/number"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            motion:layout_constraintHorizontal_bias="0.044"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/name"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.006"
            motion:layout_constraintTop_toBottomOf="@+id/imageButton"
            motion:layout_constraintHorizontal_bias="0.0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/anotherEnd" >
        <Constraint
            android:id="@+id/imageButton"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="84dp" />
        <Constraint
            android:id="@+id/status"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            motion:layout_constraintBottom_toTopOf="@+id/imageButton"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/species"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/name"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/number"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            motion:layout_constraintHorizontal_bias="0.044"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/name"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.006"
            motion:layout_constraintTop_toBottomOf="@+id/imageButton"
            motion:layout_constraintHorizontal_bias="0.0" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/anotherStart"
        motion:constraintSetEnd="@+id/anotherEnd"
        motion:autoTransition="jumpToStart"
        android:id="@+id/anotherTransition">
        <KeyFrameSet >
            <KeyAttribute
                motion:motionTarget="@+id/imageButton"
                motion:framePosition="1"
                android:alpha="1"
                android:scaleX="1.5"
                android:scaleY="1.5">
                <CustomAttribute
                    motion:attributeName="colorFilter"
                    motion:customColorValue="@color/morty" />
            </KeyAttribute>
        </KeyFrameSet>
    </Transition>
</MotionScene>